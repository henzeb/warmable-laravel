{
    "name": "henzeb/warmable-laravel",
    "description": "A cache warmer for the Laravel Framework",
    "keywords": [
        "henzeb",
        "warmable",
        "warmup",
        "warmer",
        "cache",
        "warm",
        "up",
        "performance",
        "speed",
        "speedup",
        "heat",
        "preheat",
        "laravel",
        "illuminate"
    ],
    "homepage": "https://github.com/henzeb/warmable-laravel",
    "license": "AGPL-3.0-only",
    "type": "library",
    "authors": [
        {
            "name": "Henze Berkheij",
            "email": "henzeberkheij@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "henzeb/warmable": "^v1.1.0",
        "illuminate/support": "^9.0|^10"
    },
    "require-dev": {
        "infection/infection": "*",
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9|^10"
    },
    "autoload": {
        "psr-4": {
            "Henzeb\\Warmable\\Illuminate\\": "src/Illuminate"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Henzeb\\Warmable\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage-txt": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-text",
        "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-html coverage",
        "test-dox": "vendor/bin/phpunit --testdox"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
